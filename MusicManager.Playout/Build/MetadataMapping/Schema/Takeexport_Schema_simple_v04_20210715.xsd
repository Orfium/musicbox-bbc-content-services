<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:vcs="http://ExternalFunctions.vcs.de" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Schema for Metadatafiles for Take related Export and import by dira!Connect. AEP Context-->
	<!-- Version tailored for music import by Jasred Thomas 2020-11-12-->
	<!-- v02 20210715: GENE_MREPORT_STATUS and GENE_MUSIC_CODE are now mandatory, more notes added on relational Persons model, notes on GENE_PERFORMERS and GENE_COMPOSERS added -->
	<xs:element name="EXPORT">
		<xs:annotation>
			<xs:documentation>The root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="IMEX_ID" type="xs:string" nillable="false" minOccurs="0">
					<xs:annotation>
						<xs:documentation>IMEX_ID is not a metadate of the exported take, but of the export process</xs:documentation>
						<xs:documentation>Export_AEP.xsl in current Version (18.02.2013 does not provide this date, so alwaysOccurs="0"</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TAKE"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TAKE">
		<xs:annotation>
			<xs:documentation>container for each take being exported normally only one take par file.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="GENERIC"/>
				<xs:element ref="TAKE_PERSONS"/>
				<xs:element ref="BASIC_TEXT" minOccurs="0"/>
				<xs:element ref="AUDIO_MARKERS" minOccurs="0"/>
				<xs:element ref="GENEALOGY" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="AUDIO_MARKERSType">
		<xs:sequence>
			<xs:element ref="AUDIO_MARKER" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AUDIO_MARKERType">
		<xs:sequence>
			<xs:element name="AMA_FUNCTION" type="xs:string"/>
			<xs:element name="AMA_MARK_IN" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_MARK_OUT" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_FADE_IN" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_FADE_OUT" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_INTRO1" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_INTRO2" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_INTRO3" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_OUTRO1" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_OUTRO2" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_OUTRO3" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_PREV_LINK" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_NEXT_LINK" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_REFRAIN1" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_REFRAIN2" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_REFRAIN3" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_LAST_MOD_USER" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_FADE_CURVE1" type="xs:string" minOccurs="0"/>
			<xs:element name="AMA_FADE_CURVE2" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BTXType">
		<xs:annotation>
			<xs:documentation>type for the collection of standard text objects.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BTX_REMARK" type="xs:string" nillable="true"/>
			<xs:element name="BTX_MOD_BEGIN" type="xs:string" nillable="true"/>
			<xs:element name="BTX_MOD_END" type="xs:string" nillable="true"/>
			<xs:element name="BTX_INFOTEXT" type="xs:string" nillable="true"/>
			<xs:element name="BTX_DIRECTION" type="xs:string" nillable="true"/>
			<xs:element name="BTX_ABSTRACT" type="xs:string" nillable="true"/>
			<xs:element name="BTX_META_CONTAINER_MISC" type="xs:string" nillable="true"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CLIPType">
		<xs:annotation>
			<xs:documentation>ClipMetadata appear in the same substructure as TakeMetadata</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CLIP_INFO" minOccurs="0"/>
			<!--xs:element ref="METADATA_CONTAINER" minOccurs="0"/-->
			<xs:element ref="CLIP_PERSONS" minOccurs="0"/>
			<xs:element ref="CLIP_ATTACHMENT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GENEALOGY_ITEMType">
		<xs:sequence>
			<xs:element name="GENY_CREA_TIME" type="xs:string"/>
			<xs:element name="GENY_LAST_MOD_TIME" type="xs:string"/>
			<xs:element name="GENY_ID" type="xs:string"/>
			<xs:element name="GENY_TAKE_ID" type="xs:string"/>
			<xs:element name="GENY_BASIC_TYPE" type="xs:string"/>
			<xs:element name="GENY_MREPORT_STATUS" type="xs:string"/>
			<xs:element name="GENY_TAKE_START_OFFSET" type="xs:string"/>
			<xs:element name="GENY_CLIP_LENGTH" type="xs:string"/>
			<xs:element name="GENY_CLIP_START_OFFSET" type="xs:string"/>
			<xs:element name="GENY_CLIP_TARGET_LENGTH" type="xs:string"/>
			<xs:element name="GENY_LAST_MOD_USER" type="xs:string"/>
			<xs:element name="GENY_CLIP_TITLE" type="xs:string"/>
			<xs:element name="GENY_CLIP_PERSON" type="xs:string"/>
			<xs:element name="GENY_CLIP_PERFORMER" type="xs:string"/>
			<xs:element name="GENY_EXT_REFERENCE" type="xs:string"/>
			<xs:element name="GENY_SID" type="xs:string"/>
			<xs:element name="GENY_CLIP_SYS_ID" type="xs:string"/>
			<xs:element name="GENY_CLIP_MED_ID" type="xs:string"/>
			<xs:element name="GENY_CLIP_ORG_ID" type="xs:string"/>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>note: either METADATA_CONTAINER or CLIP  may exist per GENEALOGY_ITEM</xs:documentation>
				</xs:annotation>
				<xs:element ref="CLIP" minOccurs="0"/>
				<xs:element ref="METADATA_CONTAINER" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GENERICType">
		<xs:sequence>
			<xs:element name="GENE_CREA_TIME" type="xs:dateTime" nillable="false"/>
			<xs:element name="GENE_LAST_MOD_TIME" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_ROW_CREA_TIME" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_ROW_MOD_TIME" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_DEL_TIME" type="xs:dateTime" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>the datetime set for auto deletion.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GENE_REC_TIME" type="xs:dateTime" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>the time the recording was made</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GENE_VALID_START" type="xs:dateTime" nillable="true" minOccurs="0"/>
			<xs:element name="GENE_VALID_END" type="xs:dateTime" nillable="true" minOccurs="0"/>
			<xs:element name="GENE_DUE_DATE" type="xs:dateTime" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>the date time by which delivery of audio is expected (EXTA/White DIGA)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GENE_FIRST_BRC_TIME" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_LAST_BRC_TIME" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_PROD_TIME" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_PUB_TIME" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_PLANNED_SCHED_TIME" type="xs:dateTime" nillable="false"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>the datetime of the next scheduled playout</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GENE_FT_REFNO_DATE" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_REG_DATE" type="xs:dateTime" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_MEDIA_EDIT_DATE" type="xs:dateTime" nillable="false"
				minOccurs="0"/>
			<xs:element name="GENE_ID" type="xs:string" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Take ID - this must be present on an export but can be absent on an import, as it will be generated by the importing dira workgroup.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GENE_CREA_USER_ID" type="xs:string" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_LAST_MOD_USER_ID" type="xs:string" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_ROW_MOD_USER_ID" type="xs:string" nillable="false" minOccurs="0"/>
			<xs:element name="GENE_DIRA_ARCHIVE00" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_DIRA_ARCHIVE01" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_DIRA_ARCHIVE02" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_DIRA_ARCHIVE03" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_DIRA_ARCHIVE04" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_ASSET_ID" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_ALBUM_ID" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_UDT_ID" type="xs:string" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Take data card layout ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UDT_NAME" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Take data card layout name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GENE_WORK_ID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Not used currently, but could be used for ISWC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GENE_PARENT_ID" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_SCHED_STRUCT_ID" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_PROG_ID" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_ROW_STATUS" nillable="false" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ROW_STATUS$VALID#Valid"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_TYPE" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The type of asset. for Music tracks set to TYPE$MUSIC and for classical music items, set it to TYPE$CLASSIC.
</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="TYPE$GENERIC"/>
						<xs:enumeration value="TYPE$WORD"/>
						<xs:enumeration value="TYPE$MUSIC"/>
						<xs:enumeration value="TYPE$PROGRAMME"/>
						<xs:enumeration value="TYPE$VIDEO"/>
						<xs:enumeration value="TYPE$CLASSIC"/-->
						<xs:enumeration value="TYPE$WORD#Word"/>
						<xs:enumeration value="TYPE$MUSIC#Music"/>
						<xs:enumeration value="TYPE$CLASSIC#Classic"/>
						<xs:enumeration value="TYPE$GENERIC#Generic"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_WORK_STATUS" minOccurs="0" nillable="true">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="WORK_STATUS$TO BE EDITED#To Be Edited"/>
						<xs:enumeration value="WORK_STATUS$BOOKING#Booking"/>
						<xs:enumeration value="WORK_STATUS$NOT FOR TX#Not For TX"/>
						<xs:enumeration
							value="WORK_STATUS$READY FOR TRANSMISSION#Ready for Transmission"/>
						<xs:enumeration value="WORK_STATUS$CONFIRMATION#Confirmation"/>
						<xs:enumeration value="WORK_STATUS$IN PROGRESS#In Progress"/>
						<xs:enumeration value="WORK_STATUS$RAW MATERIAL#Raw Material"/>
						<xs:enumeration value="WORK_STATUS$READY TO MIX#Ready To Mix"/>
						<xs:enumeration value="WORK_STATUS$MIX COMPLETED#Mix Completed"/>
						<xs:enumeration value="WORK_STATUS$RIGHTS_FREE#Rights Free"/>
						<xs:enumeration value="WORK_STATUS$TEMP EDITS MADE#Temp Edits Made"/>
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_LOGSTORE" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>this list is system specific and not generic for all AEP systems. Until further notice, this field is ignored, and AM_RADIO4_R4WIP for import is used by default</xs:documentation>
					<xs:documentation>for export it's not predictable from which logstore the according take be exported, so an according enumeration may be included system specific</xs:documentation>
				</xs:annotation>
				<!--xs:simpleType>
	
				<xs:restriction base="xs:string">
					<xs:enumeration value="LOGSTORE$AM_RADIO4_R4WIP"/>
				</xs:restriction>
			</xs:simpleType-->
			</xs:element>
			<xs:element name="GENE_PROC_STATE" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_FORM_CLASS" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="FORM_CLASS$JINGLE"/>
					<xs:enumeration value="FORM_CLASS$NEWS"/>
					<xs:enumeration value="FORM_CLASS$SPORT"/>
					<xs:enumeration value="FORM_CLASS$OTHER"/-->
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_LANGUAGE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_PROD_TYPE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_PLAY_BEGIN" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_PLAY_END" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="PLAY_END$FADE#Fade"/>
						<xs:enumeration value="PLAY_END$COLD#Cold"/>
						<xs:enumeration value="PLAY_END$QUICK#Quick"/>
						<xs:enumeration value="PLAY_END$APPLAUSE#Applause"/>
						<xs:enumeration value="PLAY_END$WARNING#Warning"/>
						<xs:enumeration value="PLAY_END$NO ENTRY#No Entry"/>
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_PLAY_MODE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="PLAY_MODE$STOP_ALL"/--> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_ROYALTIES_TYPE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="ROYALTIES_TYPE$YES"/>
						<xs:enumeration value="ROYALTIES_TYPE$NO"/>
						<xs:enumeration value="ROYALTIES_TYPE$50"/-->
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_BRC_RESTRICTION" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_RESSORT" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_MREPORT_STATUS" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The status of the Music reporting metadata. if you can supply Performers, Composers, arrangers, Label, Publisher, ISRC code, and prefferably Album, Track no, and Catalogue Number then this can be set to "MREPORT_STATUS$READY_TO_REPORT", otherwise default to "MREPORT_STATUS$DETAILS_MISSING"</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="MREPORT_STATUS$DETAILS_MISSING#Details Missing"/>
						<xs:enumeration value="MREPORT_STATUS$READY_TO_REPORT#Ready to Report"/>
						<xs:enumeration value="MREPORT_STATUS$NOT_ REP#Not to be Reported"/>
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_DIGI_SOURCE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_BRC_CATEGORY" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_POOL" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_FUNCTION" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_NETWORK" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_SRC_SITE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_DEPARTMENT" type="xs:string" minOccurs="0"/>
			<xs:element name="GENE_SCHED_TYPE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="SCHED_TYPE$DEFAULT"/>
						<xs:enumeration value="SCHED_TYPE$EXTA"/>
						<xs:enumeration value="SCHED_TYPE$DIGA"/>
						<xs:enumeration value="SCHED_TYPE$MODF"/-->
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_PRG_TYPE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_PRG_CONTENT" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_REC_STATUS" minOccurs="0" nillable="true">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="REC_STATUS$WAITING#Waiting"/>
						<xs:enumeration value="REC_STATUS$RECORDING#Recording"/>
						<xs:enumeration value="REC_STATUS$COMPLETED#Completed"/>
						<xs:enumeration value="REC_STATUS$ABORTED#Aborted"/>
						<xs:enumeration value="REC_STATUS$EMPTY#Empty"/>
						<xs:enumeration value="REC_STATUS$AUTORECORDING#Autorecording"/>
						<xs:enumeration value="REC_STATUS$RESTARTED#Restarted"/>
						<xs:enumeration value="REC_STATUS$COPIED#Copied"/>
						<xs:minLength value="0"/>
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_OUT_DEVICE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="OUT_DEVICE$MUSIC#Music"/>
						<xs:enumeration value="OUT_DEVICE$WORD#Word"/>
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_CONTENT_TYPE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"> </xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_START_MODE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="SE_START_MODE$MANU"/>
						<xs:enumeration value="SE_START_MODE$AUTO"/>
						<xs:enumeration value="SE_START_MODE$TIME"/-->
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_STOP_MODE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="SE_STOP_MODE$MANU"/>
						<xs:enumeration value="SE_STOP_MODE$AUTO"/>
						<xs:enumeration value="SE_STOP_MODE$TIME"/-->
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_ARCH_STATE" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<!--xs:enumeration value="ARCH_STATE$TO_RIA"/>
						<xs:enumeration value="ARCH_STATE$TO_DMI"/>
						<xs:enumeration value="ARCH_STATE$IN_RIA"/>
						<xs:enumeration value="ARCH_STATE$IN_DMI"/>
						<xs:enumeration value="ARCH_STATE$REFUSED"/>
						<xs:enumeration value="ARCH_STATE$NULL"/>
						<xs:enumeration value="ARCH_STATE$NOARCHIVE"/-->
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="GENE_MUSIC_CODE" minOccurs="1">
				<xs:annotation>
					<xs:documentation>This is the licence code - the code that tells BBC systems which rights are assoceated with the track it is mandatory, and for Commercial music it should be set to "MUSIC_CODE$C#C-Commercial" and for Library material it shoul be set to "MUSIC_CODE$M#M - Library Music"</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="MUSIC_CODE$C#C - Commercial"/>
						<xs:enumeration value="MUSIC_CODE$A#A - BBC Sound Archives"/>
						<xs:enumeration value="MUSIC_CODE$H#H - R.phonic W.shop"/>
						<xs:enumeration value="MUSIC_CODE$K#K - Com. +50 yrs old"/>
						<xs:enumeration value="MUSIC_CODE$M#M - Library Music"/>
